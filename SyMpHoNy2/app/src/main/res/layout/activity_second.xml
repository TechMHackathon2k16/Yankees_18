<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/bg">
    tools:context="crimsontyphoon.symphony.SecondActivity">



    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/imageView"
        android:src="@mipmap/d1"

        android:minHeight="@android:dimen/app_icon_size"

        android:layout_marginBottom="33dp"
        android:layout_above="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView

        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/imageView2"
        android:src="@mipmap/d2"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/imageView3"
        android:src="@mipmap/d3"
        android:layout_marginBottom="44dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DeVeLoPeRs"
        android:textStyle="bold"
        android:id="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="30dp"
        android:typeface="serif"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ekansh Srivastava"
        android:textStyle="bold"
        android:id="@+id/textView7"
        android:layout_marginTop="28dp"
        android:layout_alignTop="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:textColor="#6e1b89" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vineeta Karki"
        android:textStyle="bold"
        android:id="@+id/textView8"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:textColor="#6e1b89" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pursuing Btech(IT 3rd year)"
        android:textStyle="bold"
        android:id="@+id/textView9"
        android:textColor="#6e1b89"
        android:layout_below="@+id/textView8"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignEnd="@+id/textView7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="Pursuing Btech(IT 3rd year)"
        android:id="@+id/textView10"
        android:textColor="#6e1b89"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vaisakh Mohan"
        android:textStyle="bold"
        android:id="@+id/textView11"
        android:layout_marginTop="28dp"
        android:textColor="#6e1b89"
        android:layout_alignTop="@+id/imageView3"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pursuing Btech(IT 3rd year)"
        android:id="@+id/textView12"
        android:textStyle="bold"
        android:textColor="#6e1b89"
        android:layout_below="@+id/textView11"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10" />
</RelativeLayout>




